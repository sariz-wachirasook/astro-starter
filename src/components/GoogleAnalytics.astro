---
const GA = import.meta.env.GOOGLE_ANALYTICS;
---

<script type="text/partytown" src={`https://www.googletagmanager.com/gtag/js?id=${GA}`}></script>
<script type="text/partytown" define:vars={{ GA }}>
window.dataLayer = window.dataLayer || [];
function gtag() {
    dataLayer.push(arguments);
}
gtag('js', new Date());
gtag('config', GA);
</script>
